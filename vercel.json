{
	"buildCommand": " cd ./apps/expo && npx expo export -p web --output-dir ../../dist",
	"outputDirectory": "dist/client",
	"functions": {
		"api/index.js": {
			"runtime": "@vercel/node@3.0.11",
			"includeFiles": "dist/server/**"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/index.js"
		}
	]
}
