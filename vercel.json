{
	"buildCommand": "cd ./apps/expo && expo export -p web",
	"outputDirectory": "./apps/expo/dist/client",
	"functions": {
		"api/index.js": {
			"runtime": "@vercel/node@3.0.11",
			"includeFiles": "dist/server/**"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/index.js"
		}
	]
}
