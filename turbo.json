{
	"$schema": "https://turborepo.org/schema.json",
	"globalDotEnv": [".env"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"dotEnv": [".env"],
			"cache": true,
			"outputs": [".dist/**", "node_modules/.cache/metro/**"]
		},
		"dev": {
			"dotEnv": [".env"],
			"cache": false,
			"persistent": true
		},
		"dev:clear": {
			"dotEnv": [".env"],
			"cache": false,
			"persistent": true
		},
		"type-check": {}
	}
}
