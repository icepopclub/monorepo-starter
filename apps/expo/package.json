{
	"name": "@acme/expo",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "npx expo start",
		"dev:clear": "npx expo start -c",
		"build": "npx expo export -p web --output-dir ../../dist",
		"analyze:ios": "npx expo export --source-maps --platform ios --no-bytecode; source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
		"android": "expo run:android",
		"ios": "expo run:ios"
	},
	"dependencies": {
		"@clerk/clerk-expo": "^1.1.5",
		"@expo/vector-icons": "^14.0.2",
		"@acme/env": "workspace:*",
		"@acme/server": "workspace:*",
		"@react-native-async-storage/async-storage": "1.23.1",
		"@react-navigation/native": "^6.0.2",
		"@tanstack/react-query": "^5.37.1",
		"@trpc/client": "11.0.0-rc.374",
		"@trpc/react-query": "11.0.0-rc.374",
		"expo": "~51.0.8",
		"expo-asset": "~10.0.6",
		"expo-auth-session": "~5.5.2",
		"expo-constants": "~16.0.1",
		"expo-crypto": "~13.0.2",
		"expo-dev-client": "~4.0.14",
		"expo-document-picker": "~12.0.1",
		"expo-font": "~12.0.5",
		"expo-image-picker": "~15.0.5",
		"expo-linking": "~6.3.1",
		"expo-router": "~3.5.14",
		"expo-secure-store": "~13.0.1",
		"expo-splash-screen": "~0.27.4",
		"expo-status-bar": "~1.12.1",
		"expo-system-ui": "~3.0.4",
		"nativewind": "^2.0.11",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.74.1",
		"react-native-gesture-handler": "~2.16.1",
		"react-native-reanimated": "~3.10.1",
		"react-native-safe-area-context": "4.10.1",
		"react-native-screens": "3.31.1",
		"react-native-web": "~0.19.10",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@acme/typescript": "workspace:*",
